{
    "default": true,
    "MD003": {
        "style": "atx"
    },
    "MD007": {
        "indent": 4
    },
    "MD013": {
        "line_length": 120,
        "heading_line_length": 120,
        "code_block_line_length": 150,
        "tables": false
    },
    "MD024": {
        "siblings_only": true
    },
    "MD025": {
        "front_matter_title": ""
    },
    "MD026": {
        "punctuation": ".,;:!?"
    },
    "MD029": {
        "style": "ordered"
    },
    "MD033": {
        "allowed_elements": ["details", "summary", "br", "kbd", "mark", "abbr", "sub", "sup", "div", "span", "img", "a"]
    },
    "MD036": false,
    "MD040": true,
    "MD041": false,
    "MD046": false,
    "MD049": {
        "style": "underscore"
    },
    "MD050": {
        "style": "asterisk"
    },
    "no-hard-tabs": true,
    "whitespace": true
}
